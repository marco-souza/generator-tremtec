import { ActionType } from 'app/helpers/redux'

import { CHANGE_<%= name.toUpperCase() %>, initialState } from './constants'

export default (state = initialState, action: ActionType) => {
  switch (action.type) {
    case CHANGE_<%= name.toUpperCase() %>:
      return {
        ...state,
        <%= name.toLowerCase() %>Data: action.payload,
      }

    default:
      return state
  }
}
